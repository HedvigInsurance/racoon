{
  "name": "onboarding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --passWithNoTests",
    "e2e": "start-server-and-test 'yarn start 3000' http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test 'yarn start 3000' http://localhost:3000 cypress:headless",
    "lint": "next lint",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "download-translations": "./scripts/download-translations.sh",
    "sort-imports": "import-sort --write ./src/**/*.{ts,tsx}",
    "codegen": "graphql-codegen && prettier --write ./src/services/apollo/types.ts && import-sort --write ./src/services/apollo/types.ts && yarn lint --fix",
    "cypress": "cypress open",
    "cypress:headless": "cypress run"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@sentry/nextjs": "^6.17.6",
    "classnames": "^2.3.1",
    "cookies-next": "^2.0.4",
    "dompurify": "^2.3.5",
    "formidable": "^2.0.1",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^19.0.0",
    "marked": "^4.0.12",
    "next": "^12.0.10",
    "next-i18next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ui": "*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-operations": "^2.3.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.5",
    "@hedviginsurance/brand": "^4.2.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^2.3.3",
    "@types/formidable": "^2.0.4",
    "@types/jsdom": "^16.2.14",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-plugin-cypress": "^2.12.1",
    "import-sort-cli": "^6.0.0",
    "jest": "^27.5.1",
    "msw": "^0.36.8",
    "scripts": "*",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.5",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
