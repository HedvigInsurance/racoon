    "e2e:headless": "start-server-and-test 'yarn start --port 3001' http://localhost:3001 cypress:headless",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "download-translations": "./scripts/download-translations.sh",
    "codegen": "graphql-codegen && prettier --write ./src/services/**/{generated,types}.ts && yarn lint:fix",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
