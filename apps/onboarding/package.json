{
  "name": "onboarding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node --require ./server-preload.js ./node_modules/.bin/next start -p $PORT",
    "test": "jest --passWithNoTests",
    "e2e": "start-server-and-test 'yarn start 3000' http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test 'yarn start 3000' http://localhost:3000 cypress:headless",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "download-translations": "./scripts/download-translations.sh",
    "codegen": "graphql-codegen && prettier --write ./src/services/apollo/types.ts && yarn lint --fix",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@datadog/browser-rum": "^4.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@radix-ui/react-radio-group": "^0.1.5",
    "classnames": "^2.3.1",
    "cookies-next": "^2.0.4",
    "dd-trace": "^2.5.0",
    "dompurify": "^2.3.6",
    "formidable": "^2.0.1",
    "framer-motion": "^6.3.0",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^19.0.0",
    "marked": "^4.0.14",
    "next": "^12.1.5",
    "next-i18next": "^11.0.0",
    "pino": "^7.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ui": "*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@hedviginsurance/brand": "^5.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^2.3.3",
    "@types/formidable": "^2.0.4",
    "@types/jsdom": "^16.2.14",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.39",
    "cypress": "^9.5.4",
    "eslint": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "^27.5.1",
    "msw": "^0.39.2",
    "next-router-mock": "^0.6.5",
    "scripts": "*",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.5",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
