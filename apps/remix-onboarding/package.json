{
  "private": true,
  "name": "remix-onboarding",
  "version": "0.0.1",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "PORT=3001 node -r dotenv/config node_modules/.bin/concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "download-translations": "./scripts/download-translations.sh",
    "codegen": "graphql-codegen && prettier --write ./app/lib/generated-types.ts && import-sort --write ./app/lib/generated-types.ts"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "accept-language-parser": "^1.5.0",
    "classnames": "^2.3.1",
    "dompurify": "^2.3.5",
    "i18next": "^21.6.10",
    "jsdom": "^19.0.0",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "remix": "^1.1.3",
    "remix-i18next": "^2.1.5",
    "tiny-invariant": "^1.2.0",
    "ui": "*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@graphql-codegen/typescript-react-apollo": "^3.2.5",
    "@hedviginsurance/brand": "^4.2.0",
    "@remix-run/dev": "^1.1.3",
    "@types/accept-language-parser": "^1.5.3",
    "@types/dompurify": "^2.3.3",
    "@types/jsdom": "^16.2.14",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "dotenv": "^14.3.2",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "postcss": "^8.4.5",
    "scripts": "*",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
