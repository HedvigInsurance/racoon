{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "subType": {
      "type": "string",
      "enum": ["BRF", "RENT", "STUDENT_BRF", "STUDENT_RENT"],
      "title": "Type of apartment"
    },
    "street": {
      "type": "string",
      "title": "Street"
    },
    "zipCode": {
      "type": "string",
      "title": "Zip Code",
      "minLength": 5,
      "maxLength": 5
    },
    "city": {
      "type": "string",
      "title": "City"
    },
    "livingSpace": {
      "type": "integer",
      "title": "Living Space",
      "minimum": 0
    },
    "numberCoInsured": {
      "type": "integer",
      "title": "Number Co-Insured",
      "minimum": 0
    },
    "birthDate": {
      "type": "string",
      "title": "Birth Date",
      "format": "date"
    }
  },
  "required": ["subType", "street", "zipCode", "livingSpace", "numberCoInsured"],
  "$id": "SwedishApartment"
}
