{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "subType": {
      "type": "string",
      "enum": ["RENT", "OWN"],
      "title": "Type"
    },
    "street": {
      "type": "string",
      "title": "Street"
    },
    "zipCode": {
      "type": "string",
      "title": "Zip Code",
      "minLength": 4,
      "maxLength": 4
    },
    "city": {
      "type": "string",
      "title": "City"
    },
    "livingSpace": {
      "type": "integer",
      "title": "Living Space",
      "minimum": 0
    },
    "numberCoInsured": {
      "type": "integer",
      "title": "Number Co-Insured",
      "minimum": 0
    },
    "isYouth": {
      "type": "boolean",
      "title": "Is Youth"
    },
    "isStudent": {
      "type": "boolean",
      "title": "Is Student"
    }
  },
  "required": ["subType", "street", "zipCode", "livingSpace", "numberCoInsured"],
  "$id": "NORWEGIAN_HOME_CONTENT"
}
