{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "street": {
      "type": "string",
      "title": "Street"
    },
    "zipCode": {
      "type": "string",
      "title": "Zip Code",
      "minLength": 4,
      "maxLength": 4
    },
    "city": {
      "type": "string",
      "title": "City"
    },
    "numberCoInsured": {
      "type": "integer",
      "title": "Number Co-Insured",
      "minimum": 0
    }
  },
  "required": ["street", "zipCode", "numberCoInsured"],
  "$id": "NORWEGIAN_ACCIDENT"
}
