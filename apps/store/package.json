{
  "name": "store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 8040",
    "build": "next build",
    "start": "node --require ./server-preload.js $(yarn bin next) start",
    "test": "jest --passWithNoTests --coverage",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6008 --no-open",
    "storybook:build": "build-storybook",
    "chromatic": "npx chromatic --exit-zero-on-changes --storybook-build-dir ./storybook-static",
    "codegen": "graphql-codegen -r dotenv/config && prettier --write ./src/services/**/generated.ts && yarn lint --fix",
    "download-translations": "./scripts/download-translations.sh"
  },
