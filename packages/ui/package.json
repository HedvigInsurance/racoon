{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react --inject ./react-shim.js",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react --inject ./react-shim.js",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "start-storybook --ci -p 6006",
    "build-storybook": "build-storybook --quiet -o dist"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@emotion/babel-plugin": "^11.7.2",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "react": "^17.0.2",
    "scripts": "*",
    "tsconfig": "*",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hedviginsurance/brand": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
