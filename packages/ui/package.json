{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "tsup src/index.tsx --dts --external react --inject ./react-shim.js",
    "dev": "tsup src/index.tsx --watch --dts --external react --inject ./react-shim.js",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint src",
    "lint:fix": "eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --quiet -o dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@emotion/babel-plugin": "11.10.5",
    "@emotion/jest": "11.10.5",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@testing-library/dom": "8.17.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.0",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "babel-jest": "29.2.2",
    "babel-loader": "8.2.5",
    "jest": "29.2.2",
    "scripts": "workspace:",
    "ts-jest": "29.0.3",
    "tsconfig": "workspace:",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "tsup": "6.3.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@radix-ui/react-checkbox": "1.0.1",
    "@radix-ui/react-radio-group": "1.1.0",
    "react": "18.2.0",
    "react-animate-height": "3.0.4",
    "react-dom": "18.2.0"
  }
}
