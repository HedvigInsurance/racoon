{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [".next/**", ".turbo/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["covereage/**"],
      "dependsOn": []
    },
    "e2e": {
      "dependsOn": ["build"]
    },
    "e2e:headless": {
      "dependsOn": ["build"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "typecheck": {
      "outputs": []
    }
  }
}
