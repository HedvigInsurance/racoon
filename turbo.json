{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "onboarding#build": {
      "outputs": [".next/**"],
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_GRAPHQL_ENDPOINT",
        "$NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION",
        "$NEXT_PUBLIC_WEB_ONBOARDING_URL",
        "$NEXT_PUBLIC_DATADOG_APPLICATION_ID",
        "$NEXT_PUBLIC_DATADOG_CLIENT_TOKEN",
        "$NEXT_PUBLIC_DATADOG_VERSION",
        "$NEXT_PUBLIC_DATADOG_ENV",
        "$NEXT_PUBLIC_APP_ENV",
        "$NEXT_PUBLIC_FEATURE_HOUSE_INSURANCE",
        "$NEXT_PUBLIC_FEATURE_ACCIDENT_INSURANCE"
      ]
    },
    "store#build": {
      "outputs": [".next/**"],
      "dependsOn": [
        "^build",
        "codegen",
        "$NEXT_PUBLIC_DATADOG_APPLICATION_ID",
        "$NEXT_PUBLIC_DATADOG_CLIENT_TOKEN",
        "$NEXT_PUBLIC_DATADOG_VERSION",
        "$NEXT_PUBLIC_DATADOG_ENV",
        "$NEXT_PUBLIC_API_MOCKING",
        "$NEXT_PUBLIC_STORYBLOK_ACCESS_TOKEN",
        "$NEXT_PUBLIC_STORYBLOK_PREVIEW_TOKEN",
        "$NEXT_PUBLIC_GRAPHQL_ENDPOINT"
      ]
    },
    "market-web#build": {
      "outputs": [".next/**"],
      "dependsOn": ["^build", "$STORYBLOK_API_KEY", "$STORYBLOK_PREVIEW_SECRET"]
    },
    "fake-api#build": {
      "outputs": [".next/**"],
      "dependsOn": ["^build", "codegen"]
    },
    "onboarding#test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "store#test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["codegen"]
    },
    "e2e": {
      "dependsOn": ["build"]
    },
    "e2e:headless": {
      "dependsOn": ["build"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "onboarding#dev": {
      "cache": false,
      "dependsOn": ["codegen"]
    },
    "store#dev": {
      "cache": false,
      "dependsOn": ["codegen"]
    },
    "clean": {
      "cache": false
    },
    "storybook": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "storybook:build": {
      "outputs": ["storybook-static/**"],
      "dependsOn": ["^build", "codegen"]
    },
    "chromatic": {
      "dependsOn": ["storybook:build"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "store#typecheck": {
      "dependsOn": ["codegen", "^build"],
      "outputs": []
    },
    "onboarding#codegen": {
      "inputs": ["src/graphql/*.graphql"],
      "outputs": ["src/services/**/generated.ts", "src/services/apollo/types.ts"],
      "dependsOn": ["$NEXT_PUBLIC_GRAPHQL_ENDPOINT"]
    },
    "store#codegen": {
      "inputs": ["src/graphql/*.graphql"],
      "outputs": ["src/services/**/generated.ts"],
      "dependsOn": ["$NEXT_PUBLIC_GRAPHQL_ENDPOINT"]
    }
  }
}
