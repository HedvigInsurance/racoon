{
  "name": "racoon",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "e2e:headless": "turbo run e2e:headless",
    "clean": "turbo run clean && rm -rf node_modules",
    "start": "yarn workspace onboarding start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook": "yarn workspace ui storybook"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.1.2"
  }
}
